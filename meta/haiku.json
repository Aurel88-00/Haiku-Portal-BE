[
  {
    "title": "Old Pond",
    "author": "60c72b2f9a7d1b3f8c0e2a11",
    "lines": [
      "Old pond, still as glass,",
      "a frog jumps into the quiet,",
      "splash! A ripple spreads."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1686
  },
  {
    "title": "A Cicada's Cry",
    "author": "60c72b2f9a7d1b3f8c0e2a11",
    "lines": [
      "A cicada's cry:",
      "no sign it is about to die,",
      "on the path, it lies."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1690
  },
  {
    "title": "First Winter Rain",
    "author": "60c72b2f9a7d1b3f8c0e2a11",
    "lines": [
      "First winter rain -",
      "the world is now grey, no life,",
      "a cold, lonely view."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1684
  },
  {
    "title": "The Light of a Candle",
    "author": "60c72b2f9a7d1b3f8c0e2a12",
    "lines": [
      "The light of a candle",
      "is transferred to another candle -",
      "spring twilight."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1775
  },
  {
    "title": "Coolness of the Evening",
    "author": "60c72b2f9a7d1b3f8c0e2a12",
    "lines": [
      "Coolness of the evening",
      "the sound of a single frog",
      "through the fields."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1780
  },
  {
    "title": "A Poppy Blooms",
    "author": "60c72b2f9a7d1b3f8c0e2a12",
    "lines": ["A poppy blooms", "now a butterfly waits,", "a new beginning."],
    "originalLanguage": "Japanese",
    "translationSource": "Various English translations",
    "yearWritten": 1765
  },
  {
    "title": "The Morning Glory",
    "author": "60c72b2f9a7d1b3f8c0e2a13",
    "lines": [
      "The morning glory",
      "tied to my well-bucket,",
      "I beg for water."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1820
  },
  {
    "title": "Don't Kill the Fly",
    "author": "60c72b2f9a7d1b3f8c0e2a13",
    "lines": [
      "Don't kill the fly!",
      "look, it's just waving its hands,",
      "a prayer for mercy."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1810
  },
  {
    "title": "A World of Dew",
    "author": "60c72b2f9a7d1b3f8c0e2a13",
    "lines": [
      "A world of dew,",
      "and within every dewdrop,",
      "a world of struggle."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1825
  },
  {
    "title": "The Persimmon",
    "author": "60c72b2f9a7d1b3f8c0e2a14",
    "lines": [
      "The persimmon,",
      "what a sweet and juicy fruit,",
      "taste of autumn."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1895
  },
  {
    "title": "Even the Sparrows",
    "author": "60c72b2f9a7d1b3f8c0e2a14",
    "lines": [
      "Even the sparrows",
      "are wearing dust in the heat,",
      "a summer day."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1898
  },
  {
    "title": "A Snail's Pace",
    "author": "60c72b2f9a7d1b3f8c0e2a14",
    "lines": [
      "A snail's pace,",
      "it reaches the mountain peak,",
      "slow but steady."
    ],
    "originalLanguage": "Japanese",
    "yearWritten": 1890
  }
]
